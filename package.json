{
  "name": "glaxdu-react-template",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Bikram Choudhury",
    "url": "https://github.com/bikram-choudhury/glaxdu-react-template.git"
  },
  "scripts": {
    "start": "node server/app.js",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "react-scripts start",
    "start:server": "nodemon server/app.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test -- --config=jest.config.js",
    "test:watch": "react-scripts test -- --config jest.config.js --watch",
    "test:coverage": "react-scripts test -- --config jest.config.js --coverage --colors",
    "postinstall": "npm start"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "formik": "^2.0.8",
    "history": "^4.10.1",
    "jquery": "^3.5.1",
    "json-server": "^0.15.1",
    "jsonwebtoken": "^8.5.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^4.9.8",
    "react-owl-carousel": "^2.3.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-scroll-up": "^1.3.3",
    "react-share": "^4.0.1",
    "react-slick": "^0.25.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "check-prop-types": "^1.1.2",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
